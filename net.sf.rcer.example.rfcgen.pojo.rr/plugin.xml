<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="net.sf.rcer.example.rfcgen.rr.Application">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            name="%perspective.name"
            class="net.sf.rcer.example.rfcgen.rr.Perspective"
            id="net.sf.rcer.example.rfcgen.rr.perspective">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            name="%view.name"
            class="net.sf.rcer.example.rfcgen.rr.View"
            id="net.sf.rcer.example.rfcgen.rr.view">
      </view>
   </extension>

</plugin>
